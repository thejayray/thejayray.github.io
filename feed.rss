---
layout: null
---

<rss xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:atom="http://www.w3.org/2005/Atom" version="2.0">
<channel>
  <title><![CDATA[ {{ site.title }}]]></title>
  <link>{{ site.baseurl }}</link>
  <atom:link href="{{ site.baseurl }}/feed.rss" rel="self" type="application/rss+xml"/>
  <description><![CDATA[ RSS Feed - {{ site.title }} - {{ site.tagline }} ]]></description>
 {% for post in site.posts %}
  <item>
    <title><![CDATA[ {{ post.title }} ]]></title>
    <link>{{ site.baseurl }}{{ post.url }}</link>
    <guid>{{ site.baseurl }}{{ post.url }}</guid>
    <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
    <dc:creator><![CDATA[ {{ site.author.name }} ]]></dc:creator>
    <description><![CDATA[ {{ post.content | xml_escape }} ]]></description>
  </item>
 {% endfor %}
</channel>
</rss>
